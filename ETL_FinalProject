      "source": [
        "def log(mensaje):\n",
        "    times_f='%Y-%h-%d-%H:%M:%S'\n",
        "    now=datetime.now()\n",
        "    times=now.strftime(times_f)\n",
        "    with open('logfile.txt','a') as f:\n",
        "        f.write(times + ','+mensaje+'\\n')\n",
        "    "
      ]
